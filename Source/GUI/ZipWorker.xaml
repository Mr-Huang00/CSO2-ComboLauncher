<Window x:Class="CSO2_ComboLauncher.ZipWorker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Extracting" Width="300" Height="125" WindowStyle="None" AllowsTransparency="True" Background="#7F7F7F7F" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closing="Window_Closing" MouseDown="Window_MouseDown">
    <Grid>
        <TextBlock x:Name="zip" Text="Extracting" Margin="5,0,5,90" Width="290" Height="35" TextAlignment="Center" FontSize="20" Foreground="Black" FontWeight="Bold"/>
        <TextBlock x:Name="zipwkfile" Margin="5,30,5,65" Width="290" Height="30" TextAlignment="Center" FontSize="14" Foreground="Black" FontWeight="Bold"/>
        <TextBlock x:Name="zipwktext" Margin="5,50,5,35" Width="290" Height="40" TextAlignment="Center" FontSize="14" Foreground="Black" FontWeight="Bold"/>
        <ProgressBar x:Name="zipwkprogress" Margin="5,96,5,5" Width="290" Height="24"/>
    </Grid>
</Window>